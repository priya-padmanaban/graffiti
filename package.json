{
  "name": "graffiti-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:shared": "cd packages/shared && npm run build",
    "dev": "npm run build:shared && turbo run dev",
    "build": "npm run build:shared && turbo run build",
    "start": "turbo run start",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate"
  },
  "devDependencies": {
    "turbo": "^1.10.16",
    "typescript": "^5.3.3"
  }
}

